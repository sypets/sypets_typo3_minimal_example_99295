{
	"name": "sypets/sypets-example",
	"type": "typo3-cms-extension",
	"description": "TYPO3 examples",
	"autoload": {
		"psr-4": {
			"Sypets\\SypetsExample\\": "Classes/"
		}
	},
	"authors": [
		{
			"name": "Sybille Peters",
			"email": "sypets@gmx.de",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^11.5 || ^12.1.0"
	},
	"license": "GPL-2.0-or-later",
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web",
			"Package": {
				"partOfMinimalUsableSystem": true
			},
			"extension-key": "sypets_example"
		}
	}
}
